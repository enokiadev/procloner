{
  "name": "procloner",
  "version": "1.0.0",
  "description": "Advanced Website Cloning Tool with 3D Asset Support",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "install:all": "npm install && cd client && npm install",
    "setup": "npm run install:all"
  },
  "keywords": [
    "web-scraping",
    "website-cloner",
    "3d-assets",
    "spa-crawler"
  ],
  "author": "ProCloner Team",
  "license": "MIT",
  "dependencies": {
    "archiver": "^6.0.1",
    "cheerio": "^1.0.0-rc.12",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "crawlee": "^3.7.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.1",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "path": "^0.12.7",
    "playwright": "^1.40.1",
    "puppeteer": "^21.6.1",
    "sharp": "^0.33.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
